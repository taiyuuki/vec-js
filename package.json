{
  "name": "vec-js",
  "version": "0.0.1",
  "description": "A JavaScript library for working with vectors.",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/taiyuuki/vec-js"
  },
  "bugs": "https://github.com/taiyuuki/vec-js/issues",
  "homepage": "https://github.com/taiyuuki/vec-js#readme",
  "scripts": {
    "lint": "eslint --fix",
    "build": "rollup -c",
    "test": "vitest",
    "publish": "pnpm publish --registry=https://registry.npmjs.org --access=public"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "author": "taiyuuki <taiyuuki@qq.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@taiyuuki/eslint-config": "^1.4.17",
    "@types/node": "^20.17.24",
    "eslint": "^9.22.0",
    "rollup": "^4.35.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  },
  "dependencies": {},
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}